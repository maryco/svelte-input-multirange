{
  "name": "svelte-input-multirange",
  "author": "maryco",
  "version": "0.0.2",
  "license": "MIT",
  "descrption": "Multi-range input UI component for Svelte and SvelteKit",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --write . && eslint .",
    "format": "prettier --write .",
    "test": "jest",
    "prepare": "husky install",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-svelte-csf": "^2.0.11",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/svelte": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@types/jest": "^29.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.55.1",
    "svelte-check": "^2.7.1",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.6",
    "svelte-preprocess": "^4.10.6",
    "ts-jest": "^29.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "main": "InputMultiRange.svelte",
  "repository": {
    "type": "git",
    "url": "https://github.com/maryco/svelte-input-multirange.git"
  },
  "bugs": {
    "url": "https://github.com/maryco/svelte-input-multirange/issues"
  },
  "exports": {
    ".": "./InputMultiRange.svelte",
    "./RangeChanged": "./range-changed"
  },
  "volta": {
    "node": "18.9.0"
  }
}
